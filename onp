using namespace std;

bool liczba(char s){
			int tmp = (int)s;
		return (tmp>47 && tmp<58) || (tmp>96 && tmp < 123);
	}
int miejsce_w_hierarchi(string s){
	int ilosc_el = 9;
	string hierarchia[ilosc_el] = {"-U","log","pow","sin","cos","*","/","+","-"};	
   		for(int i=0;i<ilosc_el;i++)
   			if(s==hierarchia[i]) return i;
		return -1;
	}

